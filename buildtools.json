{
  "$schema": "https://mobilebuildtools.com/schemas/v2/buildtools.schema.json",
  "automaticVersioning": {
    "behavior": "PreferBuildNumber",
    "environment": "All",
    "versionOffset": 0
  },
  "appSettings": {
    "ListApp": [
      {
        "className": "Secrets",
        "accessibility": "Internal",
        "namespace": "Helpers",
        "properties": [
          {
            "name": "ListFreakApiEndpoint",
            "type": "Uri"
          },
          {
            "name": "ListFreakAuthServerEndpoint",
            "type": "Uri"
          },
          {
            "name": "AppCenterAndroidAppSecret",
            "type": "String"
          }
        ]
      }
    ]
  }
}